{
  "compilerOptions": {
    "strict": false,
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "Bundler",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "types": [
      "vite/client",
      "vike-react"
    ],
    "noEmit": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "allowImportingTsExtensions": true,
    "paths": {
      "~/*": [
        "./src/*"
      ]
    }
  },
  "exclude": [
    "dist"
  ],
  "include": [
    "../src",
    "pages"
  ]
}
