{
  "name": "@macrostrat/map-interface",
  "private": true,
  "version": "4.0.0-a1",
  "description": "Macrostrat map interface",
  "main": "index.html",
  "scripts": {
    "bundle:ui": "cd bundledDeps/ui-components && npm run build",
    "bundle:main": "webpack",
    "bundle:terrain": "cd bundledDeps/cesium-martini && npm run build",
    "bundle": "npm run-script bundle:ui && npm run-script bundle:terrain && npm run-script bundle:main",
    "bundle:dist": "npm run clean && PUBLIC_URL=/next/web/ npm run bundle:main",
    "clean": "rm -rf dist/*",
    "deploy:steno": "npm run bundle:dist && rsync -azv --delete dist/ steno:/data/projects/macrostrat/next/web/",
    "dev:ui": "cd bundledDeps/ui-components && npm run dev",
    "dev:main": "webpack --watch",
    "dev:terrain": "cd bundledDeps/cesium-martini && npm run watch",
    "dev": "npm-run-all -p --aggregate-output dev:*",
    "preinstall": "linklocal && linklocal --list | bulk -c 'npm install --suppress-warnings'"
  },
  "authors": [
    "John Czaplewski <jczaplewski@wisc.edu>",
    "Daven Quinn <daven.quinn@wisc.edu>"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@types/node": "^13.13.5",
    "babel-loader": "^8.0.4",
    "babel-polyfill": "^6.26.0",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "bulk": "^2.0.1",
    "cesium": "^1.68.0",
    "connect-history-api-fallback": "^1.5.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.3",
    "dotenv-webpack": "^1.7.0",
    "file-loader": "^6.0.0",
    "git-revision-webpack-plugin": "^3.0.6",
    "linklocal": "^2.8.2",
    "npm-run-all": "^4.1.5",
    "style-loader": "^0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "webpack": "^4.22.0",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.7.0",
    "@loadable/component": "^5.14.1",
    "@macrostrat/cesium-martini": "file:./bundledDeps/cesium-martini",
    "@macrostrat/hyper": "^1.2.6",
    "@macrostrat/ui-components": "file:./bundledDeps/ui-components",
    "@mapbox/point-geometry": "^0.1.0",
    "@mapbox/vector-tile": "^1.3.1",
    "@material-ui/core": "^3.1.0",
    "@material-ui/icons": "^3.0.1",
    "@types/cesium": "^1.67.4",
    "@types/react-redux": "^7.1.7",
    "@znemz/cesium-navigation": "^4.0.0",
    "axios": "^0.17.1",
    "classnames": "^2.2.6",
    "d3": "^4.13.0",
    "d3-format": "^1.4.4",
    "d3-jetpack": "^2.0.19",
    "d3-jetpack-module": "0.0.19",
    "geologic-patterns": "git+https://github.com/davenquinn/geologic-patterns.git",
    "gl-matrix": "^3.3.0",
    "immutability-helper": "^2.8.1",
    "isomorphic-fetch": "^2.2.1",
    "mapbox-gl": "^2.0.0",
    "multi-regl": "^1.1.1",
    "pbf": "^3.2.1",
    "prop-types": "^15.6.0",
    "react": "^16.13.1",
    "react-animate-height": "^0.10.9",
    "react-cookie": "^3.0.4",
    "react-dom": "^16.13.1",
    "react-hyperscript": "^3.2.0",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "regl": "^1.5.0",
    "resium": "^1.11.0",
    "stratiform": "file:./bundledDeps/stratiform",
    "terriajs": "^7.11.4",
    "topojson-client": "^3.0.0",
    "urijs": "^1.19.2"
  }
}
